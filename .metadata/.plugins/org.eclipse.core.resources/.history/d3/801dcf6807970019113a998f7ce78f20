package tramp;

public class Card {//カード
	//フィールド変数、カプセル化
	private String suit;//マーク
	private String num;//番号

	//ゲッターとセッター
	public String getSuit(){
		return suit;
	}
	public void setSuit(String suit) {
		this.suit=suit;
	}
	public String getNum(){
		return num;
	}
	public void setNum(String num) {
		this.num=num;
	}

		//コンストラクタ
	public Card(String suit,String num){
		this.suit=suit;
		this.num=num;

	}
	public Card() {
		this("スペード", "A");
	}

}
