package tramp;

public class Card {
	//フィールド変数、カプセル化
	private String suit;
	private String num;

	public String getSuit(){
		return suit;
	}
	public void setSuit(String suit) {
		this.suit=suit;
	}
	public String getNum(){
		return num;
	}
	public void setNum(String num) {
		this.num=num;
	}

















	public Card(String suit,String num){
		this.suit=suit;
		this.num=num;

	}
	public Card() {
		this("スペード", "A");
	}

}
